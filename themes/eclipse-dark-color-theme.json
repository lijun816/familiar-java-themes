{
	"colors": {
		"foreground": "#DDDDDD",

		"list.inactiveSelectionBackground": "#3D3D3D",
		"list.hoverBackground": "#3D3D3D",

		"editor.background": "#2F2F2F",
		"sideBar.background": "#2F2F2F",

		"editor.foldBackground": "#00000000",

		"editor.foreground": "#D9E8F7",

		"editorError.foreground": "#F74375",
		"editorWarning.foreground": "#F4C82D",
		"editorInfo.foreground": "#56C2AA",

		"tab.activeBackground": "#292929",
		"breadcrumb.background": "#292929",
		"sideBarSectionHeader.background": "#292929",
		"panel.background": "#292929",
		"editorGutter.background": "#292929",
		"editorOverviewRuler.background": "#292929",
		"minimap.background": "#292929",

		"titleBar.activeBackground": "#515658",
		"activityBar.background": "#515658",
		"menu.background": "#515658",
		"statusBar.background": "#515658",
		"statusBar.debuggingBackground": "#515658",
		"statusBar.noFolderBackground": "#515658",
		"panel.border": "#515658",
		"sideBar.border": "#515658",
		"input.border": "#515658",
		"sideBarSectionHeader.border": "#515658",

		"tab.inactiveBackground": "#464649",
		"editorGroupHeader.tabsBackground": "#464649",
		"editorGroupHeader.noTabsBackground": "#464649",
		"tab.border": "#464649",
		"tab.hoverBorder": "#464649",

		"tab.hoverBackground": "#161616",

		"tab.activeForeground": "#F7F8F8",
		"tab.inactiveForeground": "#DDDDDD",

		"editor.lineHighlightBackground": "#373737",
		"editorLineNumber.foreground": "#77919A",

		"editorSuggestWidget.border": "#515658",
		"editorHoverWidget.border": "#515658",
		"editorSuggestWidget.background": "#34393D",
		"editorHoverWidget.background": "#34393D",
		"editorSuggestWidget.foreground": "#EEEEEE",
		"editorHoverWidget.foreground": "#EEEEEE",

		"textLink.foreground": "#8AC9F2",
	},
	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.tag.xml"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"scope": [
				"keyword.other.documentation.javadoc.java"
			],
			"settings": {
				"foreground": "#9A8C7C",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"source.json comment"
			],
			"settings": {
				"foreground": "#3F5FBF"
			}
		},
		{
			"scope": [
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#BED6FF"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.localname.xml",
				"entity.other.attribute-name.xml",
				"variable.language.documentroot.xml"
			],
			"settings": {
				"foreground": "#A7EC21"
			}
		},
		{
			"scope": [
				"entity.name.tag.localname.xml",
				"entity.name.tag.xml",
				"keyword.other.doctype.xml"
			],
			"settings": {
				"foreground": "#03A8D8",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant.character.entity.xml"
			],
			"settings": {
				"foreground": "#DD2867"
			}
		},
		{
			"scope": [
				"meta.tag.preprocessor.xml punctuation.definition.tag.xml"
			],
			"settings": {
				"foreground": "#0394BE"
			}
		},
		{
			"scope": [
				"entity.name.tag.yaml",
				"source.yaml constant.language",

				"markup.heading.markdown",
				"markup.inline.raw.string.markdown",
				"markup.fenced_code.block.markdown",

				"source.editorconfig keyword.other.definition",
				"source.editorconfig constant.language.boolean"
			],
			"settings": {
				"foreground": "#569CD6"
			}
		},
		{
			"scope": [
				"string.unquoted.plain.out.yaml",
				"string.quoted.double.yaml",
				"string.quoted.single.yaml",

				"source.editorconfig constant.language"
			],
			"settings": {
				"foreground": "#CE9178"
			}
		},
		{
			"scope": [
				"source.yaml constant.numeric",

				"source.editorconfig constant.numeric"
			],
			"settings": {
				"foreground": "#B5CEA8"
			}
		},
		{
			"scope": [
				"markup.heading.markdown"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"meta.embedded.block"
			],
			"settings": {
				"foreground": "#D9E8F7"
			}
		},
		{
			"scope": [
				"markup.quote.markdown"
			],
			"settings": {
				"foreground": "#265691"
			}
		},
		{
			"scope": [
				"entity.name.type.class.java",
				"entity.name.type.enum.java",
				"entity.other.inherited-class.java",
				"storage.type.java",
				"variable.other.object.java",
				"storage.type.generic.java",
				"storage.type.object.array.java"
			],
			"settings": {
				"foreground": "#1290C3"
			}
		},
		{
			"scope": [
				"constant.other.enum.java"
			],
			"settings": {
				"foreground": "#66E1F8",
				"fontStyle": "italic bold"
			}
		},
		{
			"scope": [
				"punctuation.definition.annotation.java",
				"storage.type.annotation.java"
			],
			"settings": {
				"foreground": "#A0A0A0",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"variable.other.definition.java"
			],
			"settings": {
				"foreground": "#F2F200"
			}
		},
		{
			"scope": [
				"variable.parameter.java"
			],
			"settings": {
				"foreground": "#79ABFF"
			}
		},
		{
			"scope": [
				"entity.name.function.java"
			],
			"settings": {
				"foreground": "#A7EC21"
			}
		},
		{
			"scope": [
				"storage.modifier.java",
				"storage.modifier.extends.java",
				"storage.modifier.implements.java",
				"storage.type.primitive.java",
				"storage.type.primitive.array.java",
				"constant.language.java",
				"keyword.other.import.java",
				"keyword.other.package.java",
				"keyword.control.java",
				"keyword.control.new.java",
				"keyword.control.try.java",
				"keyword.control.catch.java",
				"keyword.control.finally.java",
				"keyword.control.throw.java",
				"keyword.operator.instanceof.java",
				"variable.language.java",
				"variable.language.this.java",
				"variable.other.object.property.java"
			],
			"settings": {
				"foreground": "#CC6C1D"
			}
		},
		{
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#6897BB"
			}
		},
		{
			"scope": [
				"constant.numeric.json"
			],
			"settings": {
				"foreground": "#D25242"
			}
		},
		{
			"scope": [
				"constant.language.json"
			],
			"settings": {
				"foreground": "#03A8D8"
			}
		},
		{
			"scope": [
				"string.quoted.double",
				"string.quoted.single",
			],
			"settings": {
				"foreground": "#17C6A3"
			}
		},
		{
			"scope": [
				"string.quoted.double.json"
			],
			"settings": {
				"foreground": "#FFC600",
				"fontStyle": "italic bold"
			}
		},
		{
			"scope": [
				"string.quoted.double.xml"
			],
			"settings": {
				"fontStyle": "italic"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"*:java": {
			"foreground": "#D9E8F7",
			"fontStyle": ""
		},
		"keyword:java": "#CC6C1D",
		"keyword.documentation:java": {
			"foreground": "#9A8C7C",
			"fontStyle": "bold"
		},
		"modifier:java": "#CC6C1D",
		"class:java": "#1290C3",
		"class.abstract:java": "#3EABE6",
		"class.importDeclaration:java": "#D9E8F7",
		"interface:java": "#80F2F6",
		"interface.importDeclaration:java": "#D9E8F7",
		"enum:java": {
			"foreground": "#CC81BA",
			"fontStyle": "italic"
		},
		"enum.importDeclaration:java": {
			"foreground": "#D9E8F7",
			"fontStyle": ""
		},
		"annotation:java": {
			"foreground": "#A0A0A0",
			"fontStyle": "italic"
		},
		"annotation.importDeclaration:java": {
			"foreground": "#D9E8F7",
			"fontStyle": ""
		},
		"*.typeArgument:java": "#B166DA",
		"class.generic.typeArgument:java": "#1290C3",
		"class.abstract.generic.typeArgument:java": "#3EABE6",
		"interface.generic.typeArgument:java": "#80F2F6",
		"enum.generic.typeArgument:java": {
			"foreground": "#CC81BA",
			"fontStyle": "italic"
		},
		"annotation.generic.typeArgument:java": {
			"foreground": "#A0A0A0",
			"fontStyle": "italic"
		},
		"typeParameter:java": "#BFA4A4",
		"function:java": "#A7EC21",
		"function.abstract:java": "#80F6A7",
		"function.static:java": {
			"foreground": "#96EC3F",
			"fontStyle": "italic"
		},
		"function.declaration:java": {
			"foreground": "#1EB540",
			"fontStyle": ""
		},
		"method:java": "#A7EC21",
		"method.abstract:java": "#80F6A7",
		"method.static:java": {
			"foreground": "#96EC3F",
			"fontStyle": "italic"
		},
		"method.declaration:java": {
			"foreground": "#1EB540",
			"fontStyle": ""
		},
		"annotationMember.abstract:java": "#EB4B64",
		"annotationMember.declaration:java": "#1EB540",
		"property:java": "#66E1F8",
		"property.static:java": {
			"foreground": "#8DDAF8",
			"fontStyle": "italic"
		},
		"property.static.readonly:java": {
			"fontStyle": "italic bold"
		},
		"enumMember:java": {
			"foreground": "#8DDAF8",
			"fontStyle": "italic bold"
		},
		"variable:java": "#F3EC79",
		"variable.declaration:java": "#F2F200",
		"parameter:java": "#79ABFF"
	}
}
