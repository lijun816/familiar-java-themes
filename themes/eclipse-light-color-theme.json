{
	"colors": {
		"foreground": "#2E3436",

		"activityBar.foreground": "#000000",
		"statusBar.foreground": "#000000",
		"editor.foreground": "#000000",

		"editor.foldBackground": "#00000000",

		"titleBar.activeBackground": "#E2E2E2",
		"menu.background": "#E2E2E2",
		"activityBar.background": "#E2E2E2",
		"statusBar.background": "#E2E2E2",
		"statusBar.debuggingBackground": "#E2E2E2",
		"statusBar.noFolderBackground": "#E2E2E2",
		"sideBarSectionHeader.background": "#E2E2E2",

		"editorGutter.background": "#F8F8F8",
		"editorOverviewRuler.background": "#F8F8F8",
		"minimap.background": "#F8F8F8",

		"editorError.foreground": "#FF0080",
		"editorWarning.foreground": "#F4C82D",
		"editorInfo.foreground": "#82A0BE",

		"statusBarItem.hoverBackground": "#FFFFFF",
		"editor.background": "#FFFFFF",
		"sideBar.background": "#FFFFFF",
		"tab.activeBackground": "#FFFFFF",
		"tab.activeBorder": "#FFFFFF",
		"breadcrumb.background": "#FFFFFF",
		"panel.background": "#FFFFFF",

		"tab.inactiveBackground": "#EEEEEE",
		"editorGroupHeader.tabsBackground": "#EEEEEE",
		"editorGroupHeader.noTabsBackground": "#EEEEEE",

		"tab.hoverBackground": "#FFFFFF",
		"tab.activeForeground": "#000000",
		"tab.inactiveForeground": "#2E3436",

		"titleBar.border": "#A0A0A0",
		"tab.border": "#A0A0A0",
		"editorGroupHeader.tabsBorder": "#A0A0A0",
		"panel.border": "#A0A0A0",
		"sideBar.border": "#A0A0A0",
		"activityBar.border": "#A0A0A0",
		"statusBar.border": "#A0A0A0",
		"input.border": "#A0A0A0",
		"sideBarSectionHeader.border": "#A0A0A0",

		"editor.lineHighlightBackground": "#E8F2FE",
		"editorLineNumber.foreground": "#787878",

		"editorSuggestWidget.border": "#A0A0A0",
		"editorHoverWidget.border": "#A0A0A0",
		"editorSuggestWidget.background": "#FFFFFF",
		"editorHoverWidget.background": "#FFFFFF",
		"editorSuggestWidget.foreground": "#000000",
		"editorHoverWidget.foreground": "#000000",

		"textLink.foreground": "#134A8E"
	},
	"tokenColors": [
		{
			"scope": [
				"source.java comment"
			],
			"settings": {
				"foreground": "#3F7F5F"
			}
		},
		{
			"scope": [
				"comment.block.javadoc.java",
				"comment"
			],
			"settings": {
				"foreground": "#3F5FBF"
			}
		},
		{
			"scope": [
				"keyword.other.documentation.javadoc.java"
			],
			"settings": {
				"foreground": "#7F9FBF",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.localname.xml",
				"entity.other.attribute-name.xml",
				"variable.language.documentroot.xml",

				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#7F007F"
			}
		},
		{
			"scope": [
				"entity.name.tag.localname.xml",
				"entity.name.tag.xml",
				"keyword.other.doctype.xml"
			],
			"settings": {
				"foreground": "#3F7F7F"
			}
		},
		{
			"scope": [
				"constant.character.entity.xml"
			],
			"settings": {
				"foreground": "#2A00FF"
			}
		},
		{
			"scope": [
				"punctuation.definition.tag.xml"
			],
			"settings": {
				"foreground": "#008080"
			}
		},
		{
			"scope": [
				"entity.name.tag.yaml",

				"source.editorconfig keyword.other.definition"
			],
			"settings": {
				"foreground": "#268BD2"
			}
		},
		{
			"scope": [
				"string.unquoted.plain.out.yaml",
				"string.quoted.double.yaml",
				"string.quoted.single.yaml",

				"source.editorconfig constant.language"
			],
			"settings": {
				"foreground": "#2AA198"
			}
		},
		{
			"scope": [
				"source.yaml constant.numeric",

				"source.editorconfig constant.numeric"
			],
			"settings": {
				"foreground": "#D33682"
			}
		},
		{
			"scope": [
				"source.yaml constant.language",

				"source.editorconfig constant.language.boolean"
			],
			"settings": {
				"foreground": "#B58900"
			}
		},
		{
			"scope": [
				"markup.italic.markdown"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.bold.markdown"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"markup.bold.markdown markup.italic.markdown"
			],
			"settings": {
				"fontStyle": "bold italic"
			}
		},
		{
			"scope": [
				"markup.heading.markdown"
			],
			"settings": {
				"foreground": "#172F47",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"markup.inline.raw.string.markdown",
				"markup.fenced_code.block.markdown"
			],
			"settings": {
				"foreground": "#4444CC"
			}
		},
		{
			"scope": [
				"markup.quote.markdown"
			],
			"settings": {
				"foreground": "#265691"
			}
		},
		{
			"scope": [
				"entity.name.type.class.java",
				"entity.name.type.enum.java",
				"entity.other.inherited-class.java",
				"storage.type.java",
				"variable.other.object.java",
				"storage.type.generic.java",
				"storage.type.object.array.java",
				"entity.name.function.java",

				"meta.embedded.block"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"scope": [
				"constant.other.enum.java"
			],
			"settings": {
				"foreground": "#0000C0",
				"fontStyle": "italic bold"
			}
		},
		{
			"scope": [
				"punctuation.definition.annotation.java",
				"storage.type.annotation.java"
			],
			"settings": {
				"foreground": "#646464"
			}
		},
		{
			"scope": [
				"variable.other.definition.java",
				"variable.parameter.java"
			],
			"settings": {
				"foreground": "#6A3E3E"
			}
		},
		{
			"scope": [
				"storage.modifier.java",
				"storage.modifier.extends.java",
				"storage.modifier.implements.java",
				"storage.type.primitive.java",
				"storage.type.primitive.array.java",
				"constant.language.java",
				"keyword.other.import.java",
				"keyword.other.package.java",
				"keyword.control.java",
				"keyword.control.new.java",
				"keyword.control.try.java",
				"keyword.control.catch.java",
				"keyword.control.finally.java",
				"keyword.control.throw.java",
				"keyword.operator.instanceof.java",
				"variable.language.java",
				"variable.language.this.java",
				"variable.other.object.property.java"
			],
			"settings": {
				"foreground": "#7F0055",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"scope": [
				"constant.numeric.json"
			],
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"scope": [
				"constant.language.json"
			],
			"settings": {
				"foreground": "#FF8C00"
			}
		},
		{
			"scope": [
				"string.quoted.double",
				"string.quoted.single"
			],
			"settings": {
				"foreground": "#2A00FF"
			}
		},
		{
			"scope": [
				"string.quoted.double.xml",
				"string.quoted.double.json"
			],
			"settings": {
				"fontStyle": "italic"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"*:java": {
			"foreground": "#000000",
			"fontStyle": ""
		},
		"keyword:java": {
			"foreground": "#7F0055",
			"fontStyle": "bold"
		},
		"keyword.documentation:java": "#7F9FBF",
		"modifier:java": {
			"foreground": "#7F0055",
			"fontStyle": "bold"
		},
		"annotation:java": "#646464",
		"annotation.importDeclaration:java": "#000000",
		"function.static:java": { "fontStyle": "italic" },
		"function.declaration:java": { "fontStyle": "" },
		"method.static:java": { "fontStyle": "italic" },
		"method.declaration:java": { "fontStyle": "" },
		"property:java": "#0000C0",
		"property.static:java": { "fontStyle": "italic"	},
		"property.static.readonly:java": { "fontStyle": "italic bold" },
		"enumMember:java": {
			"foreground": "#0000C0",
			"fontStyle": "italic bold"
		},
		"variable:java": "#6A3E3E",
		"parameter:java": "#6A3E3E"
	}
}
